@PACKAGE_INIT@

set(_NANO_supported_components core solver machine)

foreach(_comp ${NANO_FIND_COMPONENTS})
    if (NOT _comp IN_LIST _NANO_supported_components)
        set(MathFunctions_FOUND False)
        set(MathFunctions_NOT_FOUND_MESSAGE "Unsupported component: ${_comp}")
    endif()
    include("${CMAKE_CURRENT_LIST_DIR}/NANO_${_comp}Targets.cmake")
endforeach()
